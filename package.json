{
  "name": "sqlite-zod-orm",
  "version": "3.0.0",
  "description": "A fast, type-safe SQLite ORM for Bun powered by Zod schemas",
  "type": "module",
  "main": "./dist/satidb.js",
  "module": "./dist/satidb.js",
  "types": "./src/satidb.ts",
  "exports": {
    ".": {
      "import": "./dist/satidb.js",
      "types": "./src/satidb.ts"
    }
  },
  "bin": {
    "sqlite-zod-orm": "./dist/satidb.js"
  },
  "scripts": {
    "build": "bun run ./src/build.ts",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "keywords": [
    "sqlite",
    "database",
    "bun",
    "typescript",
    "type-safe",
    "orm",
    "sql"
  ],
  "author": "7flash",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:7flash/satidb.git"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "zod": "^3.25.67"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}